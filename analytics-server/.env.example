# Analytics Server - Environment Variables

# Copy this file to .env and customize for your environment
# DO NOT commit .env to version control

# Server Settings
PORT=3000
CORS_ORIGIN=*

# Security
# IMPORTANT: Change this secret key in production!
SECRET_KEY=analytics-secret-key

# Feature Toggles
# Set to 'false' to disable features
ENABLE_ENCRYPTION=true
ENABLE_FEISHU_ALERTS=true

# Feishu Integration
FEISHU_WEBHOOK=https://open.feishu.cn/open-apis/bot/v2/hook/your-webhook-url-here
FEISHU_MIN_INTERVAL=5000
FEISHU_MAX_PER_MINUTE=10

# Database
DB_PATH=./db/analytics.db
